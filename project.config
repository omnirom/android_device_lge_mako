[access]
	inheritFrom = All-Projects
[project]
[access "refs/heads/*"]
	push = +force group Mako Maintainers
	label-Code-Review = -2..+2 group Mako Maintainers
	label-Verified = -1..+1 group Mako Maintainers
	submit = group Mako Maintainers
	create = group Mako Maintainers
	pushMerge = group Mako Maintainers
[access "refs/for/refs/heads/*"]
	pushMerge = group Mako Maintainers
